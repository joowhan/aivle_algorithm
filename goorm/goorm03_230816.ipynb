    "res = 0\n",
    "for i in range(t):\n",
    "\ta,s,b = input().split()\n",
    "\tif s =='+':\n",
    "\t\tres +=(int(a)+int(b))\n",
    "\telif s =='-':\n",
    "\t\tres +=(int(a)-int(b))\n",
    "\telif s =='/':\n",
    "\t\tres +=(int(a)//int(b))\n",
    "\telif s =='*':\n",
    "\t\tres +=(int(a)*int(b))\n",
    "print(res)"
   ]
